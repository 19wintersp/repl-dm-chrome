{
    "name": "repl DM extension",
    "description": "Adds direct messaging functionality to replit.",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {

    },
    "content_scripts": {
        "js": "./background.js"
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://repl.it/*"
    ]
}